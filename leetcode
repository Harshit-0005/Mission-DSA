class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        unordered_map<int , int>gmap;
        for(int i =0 ; i<nums.size(); i++){
            gmap[nums[i]] =i;
        }

        for(int i=0 ; i<nums.size(); i++){
            int temp = target - nums[i];
            if(gmap.find(temp) != gmap.end() && gmap[temp] != i ){
                return {i,gmap[temp]};
            }
        }

        return {};
    }
};
